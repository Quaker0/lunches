{
	"name": "sthlmlunch",
	"version": "2.0.0",
	"private": true,
	"dependencies": {
		"@material-ui/core": "4.11.3",
		"@material-ui/icons": "4.11.2",
		"@material-ui/lab": "4.0.0-alpha.57",
		"amazon-cognito-identity-js": "4.5.12",
		"aws-sdk": "^2.864.0",
		"fast-average-color": "^6.3.0",
		"fast-xml-parser": "^3.18.0",
		"jwt-decode": "3.1.2",
		"lodash": "4.17.21",
		"react": "^17.0.1",
		"react-bootstrap": "1.5.1",
		"react-dom": "^17.0.1",
		"react-helmet": "^6.1.0",
		"react-lazyload": "^3.2.0",
		"react-router-dom": "5.2.0",
		"react-scripts": "4.0.3",
		"react-vis": "1.11.7",
		"shortid": "^2.2.16",
		"yarn": "1.22.10"
	},
	"scripts": {
		"start": "REACT_APP_ENV=dev react-scripts start",
		"build": "REACT_APP_ENV=prod react-scripts build",
		"clean": "rm -rf node_modules",
    "setup": "modernizr -c modernizr-config.json && cp modernizr.js public/modernizr.js",
		"clean-prod": "aws s3 rm --recursive s3://www.sthlmlunch.se/static/js && aws s3 rm --recursive s3://www.sthlmlunch.se/static/css && aws s3 rm --recursive s3://www.sthlmlunch.se/static/media && aws s3 rm s3://www.sthlmlunch.se/ --exclude '*' --include 'precache*' --recursive",
		"deploy": "aws s3 sync --exclude \".*\" build/ s3://www.sthlmlunch.se --acl public-read",
		"lint": "eslint src --fix"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"babel-eslint": "^10.1.0",
		"eslint-plugin-react": "7.22.0",
		"node-fetch": "^2.6.1",
		"sitemap": "6.4.0"
	}
}
